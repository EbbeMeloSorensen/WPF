<Window x:Class="KeyboardInput.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KeyboardInput"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" d:DataContext="{d:DesignInstance local:MainWindowViewModel }">

    <Window.InputBindings>
        <KeyBinding Key="S" Command="{Binding DummyCommandMainWindowViewModel}"/>
    </Window.InputBindings>

    <local:SubView DataContext="{Binding SubViewModel}"/>

</Window>
